{
  "name": "ecos",
  "version": "0.0.1",
  "description": "Ecosystem simulator",
  "homepage": "https://github.com/dustinrohde/ecos",
  "repository": "https://github.com/dustinrohde/ecos",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "author": "Dustin Rohde",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.5.2",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint": "^2.1.0",
    "faucet": "0.0.1",
    "tape": "^4.4.0",
    "testling": "^1.7.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "lodash": "4.3.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "start": "watchify app.js --debug -v -t babelify -o dist/bundle.js",
    "build": "browserify -d -t babelify app.js > dist/bundle.js",
    "lint": "eslint src/*.js",
    "test": "tape -r babel-register test/*.js | faucet",
    "test:browser": "browserify -t babelify test/*.js | testling | faucet",
    "pretest": "npm run lint",
    "pretest:browser": "npm run lint",
    "prebuild": "npm run test",
    "prepublish": "npm run build"
  }
}
