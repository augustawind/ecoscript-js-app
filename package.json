{
  "name": "ecos",
  "version": "0.0.1",
  "description": "Ecosystem simulator.",
  "homepage": "https://github.com/dustinrohde/ecos",
  "main": "ecos.js",
  "directories": {
    "test": "test"
  },
  "author": "Dustin Rohde",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint": "^2.1.0",
    "faucet": "0.0.1",
    "grunt": "^0.4.5",
    "tape": "^4.4.0",
    "testling": "^1.7.1"
  },
  "dependencies": {
    "angular": "1.5.0",
    "lodash": "4.3.0"
  },
  "scripts": {
    "build": "browserify -t babelify ecos.js > dist/ecos.js",
    "lint": "eslint *.js",
    "test": "browserify test/*.js | testling | faucet",
    "test:babel": "browserify -t babelify test/*.js | testling | faucet",
    "test:node": "babel-node test/*.js | faucet",
    "pretest": "npm run lint",
    "prebuild": "npm run test",
    "prepublish": "npm run build"
  }
}
