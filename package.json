{
  "name": "ecos",
  "version": "0.0.1",
  "description": "Ecosystem simulator",
  "homepage": "https://github.com/dustinrohde/ecos",
  "repository": "https://github.com/dustinrohde/ecos",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "author": "Dustin Rohde",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-plugin-transform-object-rest-spread": "6.5.0",
    "babel-preset-es2015": "6.5.0",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "eslint": "2.2.0",
    "jasmine": "2.4.1",
    "nodemon": "1.9.1",
    "watchify": "3.7.0"
  },
  "dependencies": {
    "babel-polyfill": "6.5.0",
    "express": "4.13.4",
    "express-handlebars": "3.0.0",
    "lodash": "4.3.0",
    "stampit": "2.1.1"
  },
  "babel": {
    "plugins": [
      "transform-object-rest-spread"
    ],
    "presets": [
      "es2015"
    ]
  },
  "browserify": {
    "transform": "babelify"
  },
  "scripts": {
    "start": "npm run start:client & npm run start:server",
    "start:client": "watchify client/app.js -o public/bundle.js -dv",
    "start:server": "nodemon --exec babel-node -- index.js",
    "build": "browserify -d -o www/bundle.js",
    "lint": "eslint app.js src/*.js",
    "test": "babel-node spec/specRunner.js"
  }
}
